function [  ] = comp_fft( s )
%COMP_FFT Summary of this function goes here
%   Detailed explanation goes here

figure('NumberTitle', 'off', 'Name', 'DFT vs FFT')
subplot(4,1,1); stem(fftshift(dft(real(s)))); title('Real');
hold on; stem(fftshift(fft(real(s)))); title('Real');


subplot(4,1,2); stem(fftshift(dft(imag(s)))); title('Imag');
hold on; stem(fftshift(fft(imag(s)))); title('Imag');

subplot(4,1,3); stem(fftshift(dft(abs(s)))); title('Abs');
hold on; stem(fftshift(fft(abs(s)))); title('Abs');

subplot(4,1,4); stem(fftshift(dft(angle(s)))); title('Angle');
hold on; stem(fftshift(fft(abs(s)))); title('Abs');


stem4(fft(s));
legend('DFT','FFT');

end

